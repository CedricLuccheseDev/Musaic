{
  "name": "musaic",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "npx tsx tests/soundcloud.test.ts",
    "test:supabase": "npx tsx --env-file=.env tests/supabase.test.ts",
    "test:all": "npm run test && npm run test:supabase",
    "seed": "npx tsx --env-file=.env scripts/seed-database.ts",
    "release": "npm version patch && git push && git push --tags",
    "release:minor": "npm version minor && git push && git push --tags",
    "release:major": "npm version major && git push && git push --tags"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@nuxt/eslint": "^1.3.0",
    "@nuxt/fonts": "^0.11.2",
    "@nuxt/icon": "^1.12.0",
    "@nuxt/ui": "^3.1.1",
    "@nuxtjs/supabase": "^2.0.3",
    "@vueuse/motion": "^3.0.3",
    "nuxt": "^3.17.2",
    "soundcloud.ts": "^0.7.1",
    "vue": "^3.5.13",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@iconify-json/heroicons": "^1.2.3",
    "@iconify-json/simple-icons": "^1.2.62",
    "tsx": "^4.19.0",
    "typescript": "^5.8.3"
  }
}
